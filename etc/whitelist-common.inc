# common whitelist for all profiles

whitelist ~/.config/mimeapps.list
whitelist ~/.icons

# fonts
whitelist ~/.fonts
whitelist ~/.fonts.d
whitelist ~/.fontconfig
whitelist ~/.fonts.conf
whitelist ~/.fonts.conf.d

# gtk
whitelist ~/.gtkrc
whitelist ~/.gtkrc-2.0
whitelist ~/.config/gtk-3.0
whitelist ~/.themes
